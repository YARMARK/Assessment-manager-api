<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
  <changeSet id="creat_employee_info_table" author="Yar">
    <createTable tableName="employee_info">
      <column name="employee_id" type="BIGINT" defaultValueComputed="nextval('employee_seq')">
        <constraints primaryKey="true" unique="true"/>
      </column>
      <column name="first_name" type="VARCHAR">
        <constraints unique="false"/>
      </column>
      <column name="last_name" type="VARCHAR">
        <constraints unique="false"/>
      </column>
      <column name="folder_url" type="VARCHAR">
        <constraints unique="false"/>
      </column>
      <column name="date_of_incoming" type="DATE"  >
        <constraints unique="false"/>
      </column>
      <column name="last_assessment" type="DATE">
        <constraints unique="false"/>
      </column>
      <column name="next_assessment" type="DATE">
        <constraints unique="false"/>
      </column>
    </createTable>
  </changeSet>

</databaseChangeLog>